[program:postgresql]
command=/usr/lib/postgresql/9.4/bin/postgres -D /var/lib/postgresql/9.4/main -c config_file=/etc/postgresql/9.4/main/postgresql.conf
user=postgres
autorestart=true
stdout_logfile = /var/log/supervisord/%(program_name)s.log
stdout_logfile = /var/log/supervisord/%(program_name)s.error.log
